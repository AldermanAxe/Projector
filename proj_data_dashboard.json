{"save":function (){ 
		that = this; 
		name = "dashboard"; 
		$.post( "../proj/proj_data.aspx", {
			codename: name, 
			data: this.toString()
		}).done(function(msg){ 
			that._callback("...save done: " + msg); 
		}).fail(function(xhr, status, error) { 
			that._callback("...save failed"); 
		}); 
		$.post( "../proj/proj_backup.aspx", {
			codename: "dashboard", 
			data: this.toString()
		}); 
		return "save attempted"; 
},
"get_page":function (page_address){ 
		that = this; 
		if (typeof page_address == 'object'){
			pages = page_address;
			name = pages[0].site; 
			reader = that[pages[0].reader];
			style = pages[0].style;
			$.post( "http://rest-api-dev.us-east-1.elasticbeanstalk.com/v1/rss/?" + name, {
			}).done(function(msg){
				that._callback(reader(msg));
				if (page_address.length > 1){
					that.get_page(page_address.slice(1,page_address.length));
				}
			}).fail(function(xhr, status, error) { 
				that._callback("...get failed"); 
			}); 
		} else {
			name = page_address; 
			$.post( "http://rest-api-dev.us-east-1.elasticbeanstalk.com/v1/rss/?" + name, {
			}).done(function(msg){ 
				that._callback("<div style='overflow-y:scroll;height:300px;width:90%;border-color: white;border-style: solid;margin: 0px 50px 0px 50px;'>" + that.escapeHTML(msg) + "</div>");  //escapeHTML(msg) + "</div>"); 
			}).fail(function(xhr, status, error) { 
				that._callback("...get failed"); 
			}); 
		};
		return "get attempted"; 
},
"addobject":function (name, def){
	that = this;
	that[name] = eval("("+def+")");
	addfun(); 
	addobj();
	return that[name];
},
"toString":function (){
		str = "{";
		for (var property in this) {
			if (this.hasOwnProperty(property)){
				if (typeof this[property] == "object"){
					str += "\"" + property + "\":" + JSON.stringify(this[property], null, 2) + ",\n";	
				} else {
					str += "\"" + property + "\":" + this[property] + ",\n";
				}
			}
		};
		str = str.substring(0, str.length - 2);
		str += "};proj.init();proj=proj;";
		return str;
	},
"init":function (){
	this.get_page(this.rssfeeds);
},
"_callback":function mInputCallback(result){
			if (typeof result == "function"){
				runFun(result);
			} else {
				disp = document.getElementById('displaydiv');
				disp.innerHTML += "" + result;			
				disp.scrollTop = disp.scrollHeight;
			};
		},
"escapeHTML":function (string)
		{
			var pre = document.createElement('pre');
			var text = document.createTextNode( string );
			pre.appendChild(text);
			return pre.innerHTML;
		},
