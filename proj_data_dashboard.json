"save":function (){ 
		that = this; 
		name = "dashboard"; 
		$.post( "../proj/proj_data.aspx", {
			codename: name, 
			data: this.toString()
		}).done(function(msg){ 
			that._callback("...save done: " + msg); 
		}).fail(function(xhr, status, error) { 
			that._callback("...save failed"); 
		}); 
		$.post( "../proj/proj_backup.aspx", {
			codename: "dashboard", 
			data: this.toString()
		}); 
		return "save attempted"; 
},
"toString":function (){
		str = "{";
		for (var property in this) {
			if (this.hasOwnProperty(property)){
				if (typeof this[property] == "object"){
					str += "\"" + property + "\":" + JSON.stringify(this[property]) + ",\n";	
				} else {
					str += "\"" + property + "\":" + this[property] + ",\n";
				}
			}
		};
		str = str.substring(0, str.length - 2);
		str += "}";
		return str;
	},
"_callback":function mInputCallback(result){
