{"save":function (){ 
		that = this; 
		name = "dashboard"; 
		$.post( "../proj/proj_data.aspx", {
			codename: name, 
			data: this.toString()
		}).done(function(msg){ 
			that._callback("...save done: " + msg); 
		}).fail(function(xhr, status, error) { 
			that._callback("...save failed"); 
		}); 
		$.post( "../proj/proj_backup.aspx", {
			codename: "dashboard", 
			data: this.toString()
		}); 
		return "save attempted"; 
},
"get_page":function (page_address){ 
		that = this; 
		name = page_address; 
		console.log(name);
		$.post( "../proj/seek.aspx", {
			pagename: name 
		}).done(function(msg){ 
			that._callback(page_address + "<br/><div style='overflow:scroll;width:80%;height:100px;border-color: white;border-style: solid;'>" + escapeHTML(msg) + "</div>");  //escapeHTML(msg) + "</div>"); 
		}).fail(function(xhr, status, error) { 
			that._callback("...get failed"); 
		}); 

		return "get attempted"; 
		
		function escapeHTML( string )
		{
			var pre = document.createElement('pre');
			var text = document.createTextNode( string );
			pre.appendChild(text);
			return pre.innerHTML;
		}
},
"addobject":function (name, def){
	that = this;
	that[name] = eval("("+def+")");
	addfun(); 
	addobj();
	return that[name];
},
"toString":function (){
		str = "{";
		for (var property in this) {
			if (this.hasOwnProperty(property)){
				if (typeof this[property] == "object"){
					str += "\"" + property + "\":" + JSON.stringify(this[property]) + ",\n";	
				} else {
					str += "\"" + property + "\":" + this[property] + ",\n";
				}
			}
		};
		str = str.substring(0, str.length - 2);
		str += "};proj.init();proj=proj;";
		return str;
	},
"init":function (){
	for (let page of this.rssfeeds) {
		this.get_page(page);
	};
},
"_callback":function mInputCallback(result){
			if (typeof result == "function"){
				runFun(result);
			} else {
				disp = document.getElementById('displaydiv');
				disp.innerHTML += "[Result]<br/>" + result + '<br/><br/>';			
				disp.scrollTop = disp.scrollHeight;
			};
		},
